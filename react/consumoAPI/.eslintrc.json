{
  "extends": ["react-app", "react-app/jest", "airbnb", "prettier"],
  "rules": {
    "react/jsx-filename-extension": "off",
    "import/prefer-default-export": "off",
    "jsx-a11y/anchor-is-valid": "off",
    "no-console": "off",
    "default-param-last": "off",
    "func-names": "off",
    "react/jsx-no-bind": "off"
  }
}
